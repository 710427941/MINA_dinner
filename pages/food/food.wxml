<view class='warp'>
  <view class='warp-box' wx:for="{{list}}" wx:key="unique">
    <view class='h2'>{{item.title}}</view>
    <view class='list'>

      <view class='item' wx:for="{{item.list}}" wx:for-item="food" wx:key="unique" bindtap='gotoFoodContent' data-id='{{food._id}}'>
        <image src='{{url}}{{food.img_url}}'></image>
        <text class='content'>{{food.title}}</text>
        <text class='price'>ï¿¥{{food.price}}</text>
      </view>

    </view>

  </view>
</view>